    "zendframework/zend-eventmanager": "~2.4.0",
    "zendframework/zend-stdlib": "~2.4.0"

  },
  "require-dev": {
    "fabpot/php-cs-fixer": "~1.11.0",
    "phpunit/phpunit": "~4.8.0",
    "phpunit/phpcov": "~2.0.0",
    "satooshi/php-coveralls": "~1.0.0",

    "nnx/zf2-test-toolkit": "~0.1.4",

